{
  "name": "ap1",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "tsc && node -L ./bin/www",
    "dev": "tsc && nodemon -L ./bin/www"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.4",
    "express": "~4.16.0",
    "express-http-context": "^1.2.2",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-express-utils": "^6.3.2",
    "nodemon": "^2.0.3",
    "querystring": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.83.0",
    "typescript": "^4.1.3",
    "typescript-collections": "^1.3.2",
    "dotenv": "latest"
  },
  "compilerOptions": {
    "skipLibCheck": true
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "src/Visas/**.{js,ts}"
    ],
    "coveragePathIgnorePatterns": [
      "/src/Visas/Infrastructure/",
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  }
}
